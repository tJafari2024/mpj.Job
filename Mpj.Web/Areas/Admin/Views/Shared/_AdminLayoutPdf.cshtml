                    <h1>
                        @ViewData["Title"]
                    </h1>
                </div>
               
            </div> *@
            <!-- /Page Header -->
            <!-- Page Body -->
            <div class="page-body">
                @RenderBody()
            </div>
        </div>
    </div>
</div>

<partial name="_AdminFooterReferences"/>
@RenderSection("scripts", false)
<partial name="_SiteMessages"/>
</body>
</html>
